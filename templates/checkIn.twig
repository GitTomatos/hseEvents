{% extends "layout.twig" %}

{% block content %}
    {% if validationErrors is empty %}
        <div class="alert alert-success text-center" role="alert">
            <p>Студент отмечен</p>
        </div>
    {% else %}
        {% for key, err in validationErrors.notRegistered %}
            <div class="alert alert-danger text-center" role="alert">
                <p>{{ err }}</p>
            </div>
        {% endfor %}
    {% endif %}
{% endblock %}